{
  "headers": [
    {
      "source": "**/*.@(html|js|vue)",
      "headers": [
        {
          "key": "Cache-Control",
          "Access-Control-Allow-Origin": "westteam.nulstar.com",
          "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
          "Access-Control-Allow-Headers": "User-Agent,X-Requested-With,Cache-Control,Content-Type",
          "ssl-cert": "/etc/letsencrypt/archive/westteam.nulstar.com/fullchain1.pem",
          "ssl-key": "/etc/letsencrypt/archive/westteam.nulstar.com/privkey1.pem"
        }
      ]
    },
    {
      "source": "**/*.@(jpg|jpeg|gif|png)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=7200",
          "Access-Control-Allow-Origin": "westteam.nulstar.com"
        }
      ]
    }
  ]
}
